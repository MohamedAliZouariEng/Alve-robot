<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="alve">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import alve urdf file -->
    <xacro:include filename="$(find alve_description)/urdf/alve.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="alve.ros2_control.xacro" />


    <xacro:alve_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
